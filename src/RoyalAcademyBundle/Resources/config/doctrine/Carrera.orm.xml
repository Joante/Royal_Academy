<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="RoyalAcademyBundle\Entity\Carrera" table="carrera">
    <indexes>
      <index name="fk_Carrera_Sede1_idx" columns="Sede_idSede"/>
    </indexes>
    <id name="idcarrera" type="integer" column="idCarrera">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="Nombre" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="sedesede" target-entity="Sede" fetch="LAZY">
      <join-columns>
        <join-column name="Sede_idSede" referenced-column-name="idSede"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="materiamateria" target-entity="Materia" mapped-by="carreracarrera" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
