<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="RoyalAcademyBundle\Entity\Pregunta" table="pregunta">
    <indexes>
      <index name="fk_Pregunta_Examen1_idx" columns="Examen_idExamen"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="UNIQ_AEE0E1F786387D5E" columns="Examen_idExamen"/>
    </unique-constraints>
    <id name="idpregunta" type="integer" column="idPregunta">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="descripcion" type="string" column="descripcion" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <many-to-one field="examenexamen" target-entity="Examen" fetch="LAZY">
      <join-columns>
        <join-column name="Examen_idExamen" referenced-column-name="idExamen"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
