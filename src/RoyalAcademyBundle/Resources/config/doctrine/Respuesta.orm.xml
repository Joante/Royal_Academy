<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="RoyalAcademyBundle\Entity\Respuesta" table="respuesta">
    <indexes>
      <index name="fk_respuesta_Pregunta1_idx" columns="Pregunta_idPregunta"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="UNIQ_6C6EC5EE45A52D35" columns="Pregunta_idPregunta"/>
    </unique-constraints>
    <id name="idrespuesta" type="integer" column="idrespuesta">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="descripcion" type="string" column="descripcion" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="escorrecta" type="boolean" column="esCorrecta" nullable="true"/>
    <many-to-one field="preguntapregunta" target-entity="Pregunta" fetch="LAZY">
      <join-columns>
        <join-column name="Pregunta_idPregunta" referenced-column-name="idPregunta"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="examenrealizadoexamenrealizado" target-entity="Examenrealizado" inversed-by="respuestarespuesta" fetch="LAZY">
      <join-table name="examenrealizado_has_respuesta">
        <join-columns>
          <join-column name="respuesta_idrespuesta" referenced-column-name="idrespuesta"/>
        </join-columns>
        <inverse-join-columns>
          <join-column name="ExamenRealizado_idExamenRealizado" referenced-column-name="idExamenRealizado"/>
        </inverse-join-columns>
      </join-table>
    </many-to-many>
  </entity>
</doctrine-mapping>
