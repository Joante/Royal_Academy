<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="RoyalAcademyBundle\Entity\Fechaexamen" table="fechaexamen">
    <indexes>
      <index name="fk_FechaExamen_Sede1_idx" columns="Sede_idSede"/>
      <index name="fk_FechaExamen_Materia1_idx" columns="Materia_idMateria"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="UNIQ_4F874DD2B8365375" columns="Materia_idMateria"/>
      <unique-constraint name="UNIQ_4F874DD219A83F38" columns="Sede_idSede"/>
    </unique-constraints>
    <id name="idfechaexamen" type="integer" column="idFechaExamen">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="cantmax" type="integer" column="CantMax" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="cantactual" type="integer" column="CantActual" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="fecha" type="date" column="Fecha" nullable="false"/>
    <many-to-one field="sedesede" target-entity="Sede" fetch="LAZY">
      <join-columns>
        <join-column name="Sede_idSede" referenced-column-name="idSede"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="materiamateria" target-entity="Materia" fetch="LAZY">
      <join-columns>
        <join-column name="Materia_idMateria" referenced-column-name="idMateria"/>
      </join-columns>
    </many-to-one>
    <many-to-many field="alumnoalumno" target-entity="Alumno" mapped-by="fechaexamenfechaexamen" fetch="LAZY"/>
  </entity>
</doctrine-mapping>
