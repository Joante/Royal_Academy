{% extends 'base.html.twig' %}

{% block body %}
    <h1>Examenrealizado</h1>

    <table>
        <tbody>
            <tr>
                <th>Estacompletado</th>
                <td>{% if examenrealizado.estacompletado %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Nota</th>
                <td>{{ examenrealizado.nota }}</td>
            </tr>
            <tr>
                <th>Informado</th>
                <td>{% if examenrealizado.informado %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Turno</th>
                <td>{{ examenrealizado.turno }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{% if examenrealizado.fecha %}{{ examenrealizado.fecha|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Fechainicio</th>
                <td>{% if examenrealizado.fechainicio %}{{ examenrealizado.fechainicio|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Idexamenrealizado</th>
                <td>{{ examenrealizado.idexamenrealizado }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('examenrealizado_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('examenrealizado_edit', { 'idexamenrealizado': examenrealizado.idexamenrealizado }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
    <br/>
